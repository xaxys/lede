/dts-v1/;

#include "mt7986a-tl-xdr608x.dtsi"

/ {
	model = "TP-Link TL-XDR6088";
	compatible = "tplink,tl-xdr6088", "mediatek,mt7986a";
};

&switch {
	ports {
		port@1 {
			reg = <1>;
			label = "lan1";
		};

		port@2 {
			reg = <2>;
			label = "lan2";
		};

		port@3 {
			reg = <3>;
			label = "lan3";
		};

		port@5 {
			label = "lan4";
		};
	};
};
